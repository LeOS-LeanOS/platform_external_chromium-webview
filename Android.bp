license {
    name: "external_chromium-webview_license",
    visibility: [":__subpackages__"],
    license_kinds: [
        "SPDX-license-identifier-Unicode-DFS",
    ],
    // large-scale-change unable to identify any license_text files
}
soong_config_module_type {
    name: "webview_app_import",
    module_type: "android_app_import",
    config_namespace: "webview",
    value_variables: [
        "release_package_webview_version",
    ],
    properties: [
        "arch.arm.apk",
        "arch.arm64.apk",
    ],
}
webview_app_import {
    name: "webview",
    product_specific: true,
    enforce_uses_libs: false,
    preprocessed: true,
    presigned: true,
    required: [
        "libwebviewchromium_loader",
        "libwebviewchromium_plat_support",
    ],
    soong_config_variables: {
        release_package_webview_version: {
            arch: {
                arm: {
                    apk: "%s/arm/webview.apk",
                },
                arm64: {
                    apk: "%s/arm64/webview.apk",
                },
            },
        },
    },
    optional_uses_libs: [
        "androidx.window.extensions",
        "android.ext.adservices",
    ],
}
